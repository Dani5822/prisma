generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Song{
  Id Int @id @default(autoincrement())
  Cim String 
  Szerzo String
  Hossz Int
  Ar Int
  ertekeles Int
}